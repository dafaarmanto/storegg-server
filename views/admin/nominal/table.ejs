<table id="example2" class="table table-bordered table-hover">
  <thead>
  <tr>
    <th>No.</th>
    <th>Nama Koin</th>
    <th>Jumlah Koin</th>
    <th>Harga Koin</th>
    <th>Aksi</th>
  </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < nominal.length; i++){ %>
      <tr>
        <td><%= i+1 %></td>
        <td><%= nominal[i].coinName %></td>
        <td><%= nominal[i].coinQuantity %></td>
        <td><%= nominal[i].price %></td>
        <td>
          <form action="/nominal/delete/<%= nominal[i]._id %>?_method=DELETE" method="POST">
            <a href="/nominal/edit/<%= nominal[i]._id %>" class="btn btn-warning btn-sm">
              <i class="fas fa-edit"></i>
            </a>
            <button class="btn btn-danger btn-sm" onclick="return confirm('Yakin ingin menghapus?')">
              <i class="fas fa-trash"></i>
            </button>
          </form>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>